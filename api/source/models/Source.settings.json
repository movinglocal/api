{
  "connection": "default",
  "collectionName": "source",
  "info": {
    "name": "source",
    "description": "The articles are automatically imported from these datasources"
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "name": {
      "default": "",
      "unique": true,
      "type": "string",
      "required": true
    },
    "url": {
      "unique": true,
      "type": "string"
    },
    "type": {
      "default": "local",
      "type": "enumeration",
      "enum": [
        "local",
        "RSS",
        "JSON"
      ]
    },
    "articles": {
      "collection": "article",
      "via": "source"
    },
    "location": {
      "model": "location",
      "via": "sources"
    },
    "filter": {
      "default": "",
      "type": "string"
    },
    "category": {
      "default": "journalismus",
      "type": "enumeration",
      "enum": [
        "journalismus",
        "kollektive"
      ]
    },
    "users": {
      "collection": "user",
      "dominant": true,
      "via": "source",
      "plugin": "users-permissions"
    }
  }
}
